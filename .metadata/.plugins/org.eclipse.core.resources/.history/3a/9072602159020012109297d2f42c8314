package com.training.airplane;

import
import
import
import
javax.ws.rs.GET;
javax.ws.rs.Path;
javax.ws.rs.PathParam;
javax.ws.rs.Produces;
@Path("/")
public class HelloRestComponent {
@GET
@Produces("text/plain")
@Path("/greeting/{name}")
public String greet(@PathParam("name") String input)
{
return "Hello "+ input;
}
}
